    "bugs": {
        "url": "https://github.com/furkanDogu/Sector45/issues"
    },
    "homepage": "https://github.com/furkanDogu/Sector45#readme",
    "dependencies": {
        "@types/helmet": "0.0.44",
        "@types/mssql": "^4.3.1",
        "bcrypt": "^3.0.6",
        "body-parser": "^1.19.0",
        "class-validator": "^0.10.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "helmet": "^3.21.1",
        "jsonwebtoken": "^8.5.1",
        "msnodesqlv8": "^0.8.3",
        "mssql": "^5.1.0",
        "ts-node": "^8.4.1",
