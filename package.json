{
    "name": "sector45",
    "version": "1.0.0",
    "description": "Back-end application for a banking app prototype",
    "main": "src/index.js",
    "_moduleAliases": {
        "@services": "src/services"
    },
    "scripts": {
        "dev": "ts-node-dev src/index.ts",
        "db:migration:create": "ts-node node_modules\\typeorm\\cli.js migration:create -n",
        "db:migration:generate": "ts-node node_modules\\typeorm\\cli.js migration:generate -n",
        "db:migration:run": "ts-node node_modules\\typeorm\\cli.js migration:run",
        "db:create": "ts-node -- ./src/scripts/createDB",
        "db:drop": "ts-node -- ./src/scripts/dropDB"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/furkanDogu/Sector45.git"
    },
    "keywords": [],
