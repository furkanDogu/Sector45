    "devDependencies": {
        "tslint": "^5.20.0",
        "tslint-config-airbnb": "^5.11.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-loader": "^3.5.4",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "^3.6.3"
    }
